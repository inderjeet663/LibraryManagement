            <font>
                <Font size="32.0"/>
            </font>
        </Label>
        <Button fx:id="homeBtn" layoutX="651.0" layoutY="499.0" mnemonicParsing="false" onAction="#onHomeBtnClick"
                prefHeight="36.0" prefWidth="111.0" style="-fx-background-color: #cdf4d9;" text="Home">
